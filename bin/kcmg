#!/usr/bin/env raku 
use lib 'lib';
use Karabiner::CompModGenerator;

sub MAIN($config_file) {
    if !$config_file.IO.f {
        die "file $config_file does not exist";
    }

    # strip extension
    generate_file($config_file);
    my $out = $config_file.IO.extension('').basename ~ '.json';
    
}
